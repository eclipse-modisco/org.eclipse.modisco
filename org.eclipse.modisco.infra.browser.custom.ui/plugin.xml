<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.eclipse.modisco"
            name="%modisco.category.name">
      </category>
      <view
            category="org.eclipse.modisco"
            class="org.eclipse.modisco.infra.browser.custom.ui.views.CustomizationsView"
            icon="icons/uiCustom.gif"
            id="org.eclipse.modisco.infra.browser.custom.view"
            name="%customizations.view.name"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="org.eclipse.modisco"
            class="org.eclipse.modisco.infra.browser.custom.ui.wizards.NewCustomizationWizard"
            hasPages="true"
            icon="icons/modisco_new_customization.gif"
            id="org.eclipse.modisco.infra.ui.browser.custom.ui.newCustomizationWizard"
            name="%newCustomization.wizard.name"
            project="false">
      </wizard>
   </extension>
</plugin>
